{
  "name": "uberboard-cli",
  "version": "1.2.8",
  "description": "CLI application for creating and managing uberboard.io dashboards",
  "author": "uberboard.io",
  "bin": {
    "uberboard": "./bin/run"
  },
  "homepage": "https://github.com/uberboard.io/uberboard-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "git@github.com:uberboard/uberboard-cli.git",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.22",
    "@oclif/plugin-plugins": "^2.3.0",
    "@oclif/plugin-update": "^3.1.6",
    "arch": "^2.2.0",
    "axios": "^1.3.2",
    "decompress": "^4.2.1",
    "decompress-gz": "^0.0.1",
    "decompress-tar": "^4.1.1",
    "decompress-tarbz2": "^4.1.1",
    "decompress-targz": "^4.1.1",
    "decompress-unzip": "^4.0.1",
    "github-download-directory": "^3.0.0",
    "gunzip-file": "^0.1.1",
    "handlebars": "^4.7.7",
    "jszip": "^3.10.1",
    "keyv": "^4.5.2",
    "octokit": "^2.0.14",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.5",
    "@types/chai": "^4",
    "@types/decompress": "^4.2.4",
    "@types/fs-extra": "^11.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.12",
    "@types/unzipper": "^0.10.5",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^10.1.0",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "uberboard",
    "dirname": "uberboard",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-update",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    },
    "macos": {
      "identifier": "io.uberboard.cli"
    },
    "update": {
      "s3": {
        "bucket": "releases-uberboard-cli"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "postversion": "git push && git push --tags"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/uberboard.io/uberboard-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
