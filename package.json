{
  "name": "uberboard-cli",
  "version": "1.2.19",
  "description": "CLI application for creating and managing uberboard.io dashboards",
  "author": "uberboard.io",
  "bin": {
    "uberboard": "./bin/run"
  },
  "homepage": "https://github.com/uberboard.io/uberboard-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "git@github.com:uberboard/uberboard-cli.git",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^3.19.6",
    "@oclif/plugin-help": "^6.0.14",
    "@oclif/plugin-not-found": "^3.0.12",
    "@oclif/plugin-plugins": "^4.2.5",
    "@oclif/plugin-update": "^4.1.14",
    "@octokit/rest": "^20.0.2",
    "arch": "^3.0.0",
    "axios": "^1.6.7",
    "decompress": "^4.2.1",
    "decompress-gz": "^0.0.1",
    "decompress-tar": "^4.1.1",
    "decompress-tarbz2": "^4.1.1",
    "decompress-targz": "^4.1.1",
    "decompress-unzip": "^4.0.1",
    "github-download-directory": "^3.0.0",
    "gunzip-file": "^0.1.1",
    "handlebars": "^4.7.8",
    "handlebars-helpers": "^0.10.0",
    "jszip": "^3.10.1",
    "keyv": "^4.5.4",
    "octokit": "^3.1.2",
    "unzipper": "^0.10.14",
    "windows-shortcuts": "^0.1.6"
  },
  "devDependencies": {
    "@oclif/test": "^3.2.1",
    "@types/chai": "^4.3.12",
    "@types/decompress": "^4.2.7",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.20",
    "@types/unzipper": "^0.10.9",
    "chai": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-config-oclif": "^5.0.2",
    "eslint-config-oclif-typescript": "^3.0.48",
    "memfs": "^4.7.7",
    "mocha": "^10.3.0",
    "nock": "^13.5.3",
    "oclif": "^4.4.17",
    "shx": "^0.3.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "oclif": {
    "bin": "uberboard",
    "dirname": "uberboard",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-update",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    },
    "macos": {
      "identifier": "io.uberboard.cli"
    },
    "update": {
      "s3": {
        "bucket": "releases-uberboard-cli"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --fix --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha \"test/**/*.test.ts\"",
    "postversion": "git push && git push --tags"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "bugs": "https://github.com/uberboard.io/uberboard-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
